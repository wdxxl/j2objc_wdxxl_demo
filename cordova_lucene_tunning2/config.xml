<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wdxxl.cordova.lucene.tunning" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="ios-XCBuildConfiguration-J2OBJC_HOME" value="/j2objc-dist" />
        <preference name="ios-XCBuildConfiguration-FRAMEWORK_SEARCH_PATHS" value="${J2OBJC_HOME}/frameworks" />
        <preference name="ios-XCBuildConfiguration-LIBRARY_SEARCH_PATHS" value="${J2OBJC_HOME}/lib $(inherited) ${SRCROOT}/**" />
        <preference name="ios-XCBuildConfiguration-USER_HEADER_SEARCH_PATHS" value="${J2OBJC_HOME}/include ${SRCROOT}/**" />
        <preference name="ios-XCBuildConfiguration-OTHER_LDFLAGS" value="-ObjC -ljre_emul" />
    </platform>
</widget>
